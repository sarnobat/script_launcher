start n=node('45') match n-[r:CONTAINS]->c where c.title? <> \"\" return \"title\" : \" + c.title? limit 20;
start n=node(45) match n-[r:CONTAINS]->c where c.title? <> \"\" return \"title\" : \" + c.title? limit 20;
start n=node(45) match n-[r:CONTAINS]->c where c.title? <> '""' return \"title\" : \" + c.title? limit 20;
start n=node(45) match n-[r:CONTAINS]->c where c.title? <> '""' return 'title :  "' + c.title? + '"' limit 20;
